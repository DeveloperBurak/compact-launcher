{
  "name": "compact-launcher",
  "productName": "Compact Launcher",
  "description": "Compact Launcher for Your Programs",
  "version": "0.1.1",
  "private": true,
  "author": "Burak Eren Karasu <burakeren.karasu@gmail.com>",
  "copyright": "PPROGA",
  "main": "app/background.js",
  "build": {
    "appId": "com.burak.compact-launcher",
    "productName": "Compact Launcher",
    "linux": {
      "category": "Utils",
      "packageCategory": "utils",
      "target": [
        "deb",
        "snap"
      ]
    },
    "win": {
      "target": "nsis",
      "publish": []
    },
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "webpack --config=build/webpack.release.config.js --env=production && electron-builder --publish never"
  },
  "dependencies": {
    "@node-steam/vdf": "^2.1.0",
    "active-win": "^7.7.2",
    "auto-launch": "^5.0.5",
    "bootstrap": "^4.3.1",
    "electron-store": "^5.1.1",
    "jquery": "^3.4.1",
    "jquery-contextmenu": "^2.9.0",
    "node-fetch": "^2.6.1",
    "popper.js": "^1.16.1",
    "progressbar.js": "^1.1.0",
    "sweetalert2": "^11.4.18",
    "tslib": "^2.3.1",
    "winreg": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "babel-loader": "^8.0.0-beta.4",
    "babel-preset-latest-node": "^5.5.1",
    "css-loader": "^3.4.0",
    "electron": "^17.2.0",
    "electron-builder": "^22.10.5",
    "electron-installer-snap": "^5.1.0",
    "electron-rebuild": "^3.2.7",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^1.3.0",
    "postcss": "^8.1.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.0.1",
    "webpack": "^4.36.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^2.5.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
